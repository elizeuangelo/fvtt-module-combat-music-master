{
    "compilerOptions": {
        "lib": [
            "ES2022",
            "DOM",
            "DOM.Iterable"
        ],
        "target": "ES2022",
        "module": "ES2022",
        "strictNullChecks": false,
        "alwaysStrict": false,
        "removeComments": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
    },
}